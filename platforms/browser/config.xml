<?xml version='1.0' encoding='utf-8'?>
<widget id="com.himnario.mx" version="2.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Himnario LLDM</name>
    <description>
        Nueva versión del himnario lldm
    </description>
    <author email="bernagaufo@hotmail.com" href="https://www.facebook.com/villbercreativo">
        Villber México
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Orientation" value="portrait" />
</widget>
